{
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "src",
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,"allowJs": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames":true,      
    "resolveJsonModule": true,
    "moduleResolution": "node"
  }
}
